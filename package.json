{
  "name": "next-project-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "run-s",
    "lint:eslint": "eslint --ext .js,.jsx,.ts,.tsx src",
    "format": "run-s format:prettier format:eslint",
